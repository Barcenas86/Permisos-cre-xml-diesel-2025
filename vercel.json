{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "builds": [
    {
      "src": "api/permiso.js",
      "use": "@vercel/node"
    }
  ],
  "functions": {
    "api/permiso.js": {
      "maxDuration": 60,
      "memory": 1024
    }
  },
  "rewrites": [
    {
      "source": "/",
      "destination": "/api/index"
    }
  ]
}
